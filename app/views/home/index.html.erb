                                    <li>
                                       <a href="#">
                                          <% if news.admin.profile.attached? %>
                                          <%= image_tag news.admin.profile %>
                                       <% end %>
                                          <span><%= news.admin.first_name %> <%= news.admin.last_name %></span>
                                       </a>
                                    </li>
                                 </ul>
                              </div>
                           </div>
                        </div>
                  </div>
               </div>
               <% end %>
            </div>
         </section>
