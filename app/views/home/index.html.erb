                        <div class="row">
                           <div class="col-xxl-7 col-xl-8 col-lg-8 col-md-10 col-sm-10">
                              <div class="slider__content p-relative z-index-1">
                                 <span data-aos="fadeInUp" data-delay=".3s">Beta Tech Institute</span>
                                 <h2 class="slider__title" data-aos="fadeInUp" data-delay=".6s">Together We'll Explore New Things</h2>
                                 <p data-aos="fadeInUp" data-delay=".9s">We believe everyone should have the opportunity <br> to create progress through technology.</p>
                                 <div class="slider__btn" data-animation="fadeInUp" data-delay="1.1s">
                                    <%= link_to "Find Courses", courses_path, class: 'tp-btn' %>
                                 </div>
                              </div>
                           </div>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </section>
         <section class="category__area pt-105 pb-70" data-aos="fade-up">
            <div class="container">
               <div class="row">
                  <div class="col-xxl-4 col-xl-4 col-lg-4">
                     <div class="category__wrapper">
                        <div class="section__title-wrapper-2">
                           <span class="section__title-pre-2">Categories</span>
                           <h3 class="section__title-2 section__title-2-30">Explore our Popular Categories</h3>
                        </div>
                        <p>Look into yourself, get something in return as your achievement</p>
                     </div>
                  </div>
                  <div class="col-xxl-8 col-xl-8 col-lg-8">
                     <div class="category__item-wrapper">
                        <div class="row">
                           <%  @course_categories.each do |coursecategory| %>
                              <div class="col-xxl-2 col-xl-2 col-lg-3 col-md-3 col-sm-4 col-6" data-aos="fade-up" data-aos-delay="100">
                                 <div class="category__item text-center mb-45">
                                    <div class="category__icon">
                                       <% if coursecategory.image.attached? %>
                                          <%= image_tag coursecategory.image %>
                                       <% end %>
                                    </div>
                                    <div class="category__content">
                                       <h4 class="category__title">
                                          <%= coursecategory.name %>
                                       </h4>
                                    </div>
                                 </div>
                              </div>
                           <% end %>
                        </div>
                     </div>
                  </div>
               </div>
            </div>
         </section>
         <section class="course__area grey-bg-4 pt-110 pb-90" data-aos="fade-up">
            <div class="container">
              <div class="row">
                <div class="col-xxl-12">
                  <div class="section__title-wrapper-2 text-center mb-40" data-aos="fade-up">
                    <span class="section__title-pre-2">Categories</span>
                    <h3 class="section__title-2">Most Popular Courses</h3>
                  </div>
                </div>
              </div>
              <div class="row">
                <% @courses.each do |course| %>
                  <div class="col-xxl-4 col-xl-4 col-lg-4 col-md-6" data-aos="fade-up">
                    <div class="course__item-2 transition-3 white-bg mb-30 fix">
                      <div class="course__thumb-2 w-img fix">
                        <%= link_to course_path(course.slug) do %>
                                       <% if course.image.attached? %>
                                          <%= image_tag course.image %>
                                       <% end %>
                                       <% end %>
                      </div>
                      <div class="course__content-2">
                        <div class="course__top-2 d-flex align-items-center justify-content-between">
                          <div class="course__tag-2">
                            <a href="#"><%= course.course_category.name %></a>
                          </div>
                          <div class="course__price-2">
                            <span>RS <%= course.price %></span>
                          </div>
                        </div>
                        <h3 class="course__title-2">
                            <%= link_to course.title, course_path(course.slug) %>
                        </h3>
                      </div>
                    </div>
                  </div>
                <% end %>
              </div>
            </div>
          </section>
