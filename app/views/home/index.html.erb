         <section class="team__area pt-75 pb-85" data-aos="fade-up">
            <div class="container">
              <div class="row align-items-end">
                <div class="col-xxl-6 col-xl-6 col-lg-6">
                  <div class="section__title-wrapper-2 mb-40">
                    <span class="section__title-pre-2">Top Instructors</span>
                    <h3 class="section__title-2">Become A Instruction Instructor.</h3>
                  </div>
                </div>
              <div class="row">
                <% @admins.each do |admin| %>
                <div class="col-xxl-3 col-xl-3 col-lg-4 col-md-6" data-aos="fade-up">
                  <div class="team__item text-center mb-40">
                    <div class="team__thumb">
                      <div class="team__shape">
                        <img src="images/team-shape-3.png" alt="">
                      </div>
                      <% if admin.profile.attached? %>
                           <%= image_tag admin.profile %>
                        <% end %>
                      <div class="team__social transition-3">
                        <%= link_to admin.fb_link do %>
                        <i class="fa-brands fa-facebook-f"></i>
                        <% end %>
                        <%= link_to admin.twitter_link do %>
                        <i class="fa-brands fa-twitter"></i>
                        <% end %>
                        <%= link_to admin.pintrest_link do %>
                        <i class="fa-brands fa-pinterest-p"></i>
                        <% end %>
                        <%= link_to admin.linkedin do %>
                        <i class="fa-brands fa-linkedin-in"></i>
                        <% end %>
